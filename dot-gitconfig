 [alias]
 lol = log --graph --decorate --pretty=oneline --abbrev-commit
 lola = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen (%cr) %C(Bold blue)<%an>%Creset' --abbrev-commit --all
 lolat = log --graph --decorate --pretty=oneline --abbrev-commit --all --
 qlol = log HEAD~30..HEAD --decorate --pretty=oneline --abbrev-commit
[gitreview]
	username = dpoole
